{
  "name": "familie-ks-sak-frontend",
  "type": "module",
  "version": "0.0.1",
  "main": "backend/server.js",
  "repository": "https://github.com/navikt/familie-ks-sak-frontend.git",
  "author": "Team BAKS",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:backend:prod",
    "start:dev": "yarn build:backend && yarn start:backend:dev && vite dev src/frontend",
    "start:prod": "yarn build:prod && yarn start:backend:prod && vite preview dist_frontend",
    "start:backend:dev": "NODE_ENV=development node dist_backend/server.js",
    "start:backend:prod": "NODE_ENV=production node dist_backend/server.js",
    "build": "yarn build:prod",
    "build:dev": "NODE_ENV=development yarn build:backend && yarn build:frontend --mode development",
    "build:prod": "NODE_ENV=production yarn build:backend && yarn build:frontend --mode production",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "vite build src/frontend",
    "test": "vitest",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky",
    "typecheck": "tsc --noEmit -p src/backend/tsconfig.json && tsc --noEmit -p src/frontend/tsconfig.json",
    "validate": "yarn typecheck && yarn lint && yarn prettier"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --check"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.19.0",
    "@navikt/aksel-icons": "7.31.0",
    "@navikt/ds-css": "^7.31.0",
    "@navikt/ds-react": "^7.31.0",
    "@navikt/familie-backend": "^10.1.9",
    "@navikt/familie-form-elements": "^21.0.7",
    "@navikt/familie-header": "^16.1.2",
    "@navikt/familie-http": "^8.0.2",
    "@navikt/familie-ikoner": "^9.1.3",
    "@navikt/familie-logging": "^7.0.4",
    "@navikt/familie-skjema": "^8.2.28",
    "@navikt/familie-tidslinje": "^10.0.8",
    "@navikt/familie-typer": "^8.0.4",
    "@navikt/familie-visittkort": "^15.1.7",
    "@navikt/flagg-ikoner": "^10.0.0",
    "@navikt/fnrvalidator": "^2.1.5",
    "@navikt/land-verktoy": "^9.1.8",
    "@navikt/landvelger": "^10.0.0",
    "@sentry/browser": "^10.17.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "axios": "^1.12.1",
    "body-parser": "^2.2.0",
    "classnames": "^2.5.1",
    "date-fns": "4.1.0",
    "dayjs": "^1.11.18",
    "deep-equal": "^2.2.2",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "lodash": "4.17.21",
    "react": "^19.0.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.63.0",
    "react-router": "^7.9.0",
    "styled-components": "^6.1.17",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/classnames": "^2.3.1",
    "@types/deep-equal": "^1.0.1",
    "@types/eslint-plugin-jsx-a11y": "^6.9.0",
    "@types/express": "^5.0.1",
    "@types/node": "^24.6.2",
    "@types/react": "^19.0.0",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^19.0.0",
    "@types/react-router": "^5.1.20",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.5.1",
    "express-static-gzip": "^3.0.0",
    "globals": "^16.4.0",
    "husky": "^9.1.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
