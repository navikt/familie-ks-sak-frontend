{
  "name": "familie-ks-sak-frontend",
  "type": "module",
  "version": "0.0.1",
  "main": "backend/server.js",
  "repository": "https://github.com/navikt/familie-ks-sak-frontend.git",
  "author": "Team BAKS",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:backend:prod",
    "start:dev": "yarn start:lokalt-mot-preprod",
    "start-env": "yarn miljøvariabler && yarn build:backend && ENV=\"$ENV\" yarn start:backend:dev && vite dev src/frontend --mode \"$ENV\"",
    "start:lokal": "ENV=lokal yarn start-env",
    "start:hybrid": "ENV=hybrid yarn start-env",
    "start:lokalt-mot-preprod": "ENV=lokalt-mot-preprod yarn start-env",
    "start:backend:dev": "NODE_ENV=development node dist_backend/server.js",
    "start:backend:prod": "NODE_ENV=production ENV=prod node dist_backend/server.js",
    "miljøvariabler": "./hent-og-lagre-miljøvariabler.sh",
    "build": "yarn build:prod",
    "build:dev": "NODE_ENV=development yarn build:backend && yarn build:frontend --mode lokalt-mot-preprod",
    "build:preprod": "NODE_ENV=production yarn build:backend && yarn build:frontend --mode preprod",
    "build:prod": "NODE_ENV=production yarn build:backend && yarn build:frontend --mode prod",
    "build:backend": "tsc -p src/backend/tsconfig.json",
    "build:frontend": "vite build src/frontend",
    "test": "vitest",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky",
    "typecheck": "tsc --noEmit -p src/backend/tsconfig.json && tsc --noEmit -p src/frontend/tsconfig.json",
    "validate": "yarn typecheck && yarn lint && yarn prettier"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --check"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=130"
    ]
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.51.0",
    "@grafana/faro-web-sdk": "2.1.0",
    "@navikt/aksel-icons": "7.36.0",
    "@navikt/ds-css": "7.36.0",
    "@navikt/ds-react": "7.36.0",
    "@navikt/familie-backend": "10.1.16",
    "@navikt/familie-form-elements": "21.0.7",
    "@navikt/familie-header": "16.1.2",
    "@navikt/familie-http": "8.0.13",
    "@navikt/familie-ikoner": "9.1.3",
    "@navikt/familie-logging": "7.0.5",
    "@navikt/familie-skjema": "8.2.39",
    "@navikt/familie-tidslinje": "10.0.8",
    "@navikt/familie-typer": "8.0.4",
    "@navikt/flagg-ikoner": "10.0.0",
    "@navikt/fnrvalidator": "2.2.0",
    "@navikt/land-verktoy": "9.1.8",
    "@navikt/landvelger": "10.0.0",
    "@sentry/browser": "10.34.0",
    "@sentry/vite-plugin": "4.6.0",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-query-devtools": "5.90.2",
    "axios": "1.13.1",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "dayjs": "1.11.18",
    "deep-equal": "2.2.3",
    "express": "5.2.0",
    "http-proxy-middleware": "3.0.5",
    "lodash": "4.17.23",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "react-router": "7.13.0",
    "styled-components": "6.3.8",
    "tslib": "2.8.1",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@axe-core/react": "4.11.0",
    "@eslint/js": "9.39.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/classnames": "2.3.1",
    "@types/deep-equal": "1.0.4",
    "@types/eslint-plugin-jsx-a11y": "6.9.0",
    "@types/express": "5.0.1",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-router": "5.1.20",
    "@types/uuid": "11.0.0",
    "@vitejs/plugin-react": "5.1.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "express-static-gzip": "3.0.0",
    "globals": "17.3.0",
    "husky": "9.1.0",
    "jsdom": "28.0.0",
    "lint-staged": "16.2.3",
    "msw": "2.12.1",
    "prettier": "3.8.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.0",
    "vitest": "4.0.18"
  },
  "resolutions": {
    "csstype": "3.1.2"
  }
}
