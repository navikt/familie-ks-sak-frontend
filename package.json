  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/frontend/tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|svg)$": "<rootDir>/src/mockFile.js",
      "nav-(.*)-style": "<rootDir>/src/mockFile.js",
      "nav-frontend-core": "<rootDir>/src/mockFile.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@navikt)/)"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
