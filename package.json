{
  "name": "familie-ks-sak-frontend",
  "type": "module",
  "version": "0.0.1",
  "main": "backend/server.js",
  "repository": "https://github.com/navikt/familie-ks-sak-frontend.git",
  "author": "Team BAKS",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node --import=./backend/backend/register.js --es-module-specifier-resolution=node backend/backend/server.js",
    "start:dev": "tsc -p tsconfig.backend.json && NODE_ENV=development node --import=./backend/backend/register.js --es-module-specifier-resolution=node backend/backend/server.js",
    "build": "yarn build:prod",
    "build:prod": "tsc -p tsconfig.backend.json && NODE_ENV=production webpack --config ./src/webpack/webpack.prod.js",
    "test": "jest",
    "test:one": "jest -t",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "prettier-check": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky",
    "start:cypress": "tsc -p tsconfig.backend.json && NODE_ENV=production node --loader ts-node/esm --es-module-specifier-resolution=node backend/backend/cypress-server.js",
    "run:cypress": "cypress run",
    "cypress": "yarn start:cypress & yarn run:cypress",
    "typecheck": "tsc --noEmit -p src/frontend/tsconfig.json"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --check",
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.13.1",
    "@navikt/aksel-icons": "7.12.2",
    "@navikt/ds-css": "^7.12.2",
    "@navikt/ds-icons": "^2.2.0",
    "@navikt/ds-react": "^7.12.2",
    "@navikt/familie-backend": "^10.1.0",
    "@navikt/familie-form-elements": "^21.0.0",
    "@navikt/familie-header": "^15.0.0",
    "@navikt/familie-ikoner": "^9.0.0",
    "@navikt/familie-logging": "^7.0.0",
    "@navikt/familie-skjema": "^8.2.7",
    "@navikt/familie-tidslinje": "^10.0.0",
    "@navikt/familie-typer": "^8.0.0",
    "@navikt/familie-visittkort": "^15.1.0",
    "@navikt/flagg-ikoner": "^3.2.7",
    "@navikt/fnrvalidator": "^2.1.0",
    "@navikt/hoykontrast": "^3.1.0",
    "@navikt/land-verktoy": "^3.2.10",
    "@navikt/landvelger": "^3.1.5",
    "@sentry/browser": "^9.0.0",
    "@sentry/webpack-plugin": "^3.1.2",
    "axios": "^1.7.4",
    "babel-loader": "^9.2.1",
    "body-parser": "^1.20.2",
    "classnames": "^2.5.1",
    "constate": "^3.3.2",
    "date-fns": "4.1.0",
    "deep-equal": "^2.2.2",
    "dotenv": "^16.4.1",
    "express": "^5.0.1",
    "http-proxy-middleware": "^3.0.3",
    "lodash.throttle": "^4.1.1",
    "quill-delta": "^5.1.0",
    "react": "^18.3.1",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.1",
    "rxjs": "^7.8.1",
    "styled-components": "^6.1.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.20.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^15.0.2",
    "@types/classnames": "^2.3.1",
    "@types/deep-equal": "^1.0.1",
    "@types/eslint-plugin-jsx-a11y": "^6.9.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.1",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^10.0.0",
    "@types/webpack-env": "^1.18.1",
    "@types/webpack-hot-middleware": "^2.25.9",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cypress": "13.17.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-webpack-plugin": "^4.2.0",
    "express-static-gzip": "^2.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.1",
    "prettier": "^3.5.0",
    "react-refresh": "^0.16.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.24.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-merge": "^6.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/frontend/tsconfig.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|svg)$": "<rootDir>/src/mockFile.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@navikt)/)"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "resolutions": {
    "styled-components": "^6"
  }
}
